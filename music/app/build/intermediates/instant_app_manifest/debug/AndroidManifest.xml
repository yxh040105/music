<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="zj.dzh.music_list"
    android:targetSandboxVersion="2"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-sdk
        android:minSdkVersion="14"
        android:targetSdkVersion="28" />

    <application
        android:allowBackup="true"
        android:appComponentFactory="androidx.core.app.CoreComponentFactory"
        android:debuggable="true"
        android:icon="@mipmap/dzh"
        android:label="Music"
        android:roundIcon="@mipmap/dzh"
        android:supportsRtl="true"
        android:testOnly="true"
        android:theme="@style/AppTheme" >
        <activity android:name="zj.dzh.music_list.Activity.RegisterActivity" />
        <activity android:name="zj.dzh.music_list.Activity.LoginActivity" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="zj.dzh.music_list.Music.MusicActivity" />
        <activity android:name="zj.dzh.music_list.Activity.MainActivity" />
        <activity android:name="zj.dzh.music_list.Activity.SingerDescActivity" />

        <service
            android:name="zj.dzh.music_list.Music.MusicService"
            android:enabled="true"
            android:exported="true" >
        </service>
    </application>

</manifest>